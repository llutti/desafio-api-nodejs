{
  "name": "desafioapinodejs",
  "version": "1.0.0",
  "description": "Projeto \"Desafio API nodeJS\" apresentado pela Rocketseat em Agosto de 2025.",
  "author": "Luciano Cargnelutti",
  "keywords": [
    "API",
    "NodeJS",
    "Rocketseat",
    "Fastify"
  ],
  "license": "ISC",
  "homepage": "https://github.com/llutti/desafio-api-nodejs/readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llutti/desafio-api-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/llutti/desafio-api-nodejs/issues"
  },
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node --env-file .env --watch --experimental-strip-types src/main.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node --env-file .env --experimental-strip-types src/infra/database/seed.ts",
    "lint": "eslint",
    "pretest": "dotenv -e .env.test drizzle-kit migrate",
    "test": "dotenv -e .env.test vitest run --coverage"
  },
  "dependencies": {
    "@fastify/swagger": "^9.5.1",
    "@scalar/fastify-api-reference": "^1.34.2",
    "drizzle-orm": "^0.44.4",
    "fastify": "^5.5.0",
    "fastify-type-provider-zod": "^5.0.3",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  }
}
